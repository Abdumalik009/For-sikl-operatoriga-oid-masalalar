package For;

import java.util.Scanner;

public class For10 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        System.out.print("n ni kiriting (butun son, n > 0): ");
        int n = scanner.nextInt();

        int sum = 1;
        for (int i = 1; i <= n; i++) {
            sum += 1 / i;
        }

        System.out.println("S = 1 + 1/2 + 1/3 + ... + 1/" + n + " = " + sum);

        
    }
}
