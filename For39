package For;

import java.util.Scanner;

public class For39 {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);

        System.out.print("A ni kiriting: ");
        int a = input.nextInt();
        System.out.print("B ni kiriting: ");
        int b = input.nextInt();
        int sum =a-b;
        for (int i = 0; i < sum; i++) {
            System.out.println(sum);
        }

    }


}
