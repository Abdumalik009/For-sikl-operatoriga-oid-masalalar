package For;

import java.util.Scanner;

public class For19 {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);


        System.out.print("n ni kiriting (n > 0): ");
        int n = input.nextInt();

        double sum = 1;
        for (int i = 1; i <= n; i++) {

            sum *= i;
        }
        System.out.println("Ko'paytma: " + sum);

    }
}
