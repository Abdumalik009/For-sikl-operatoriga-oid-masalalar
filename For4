package For;

import java.util.Scanner;

public class For4 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.println("Konfetnini narxini kriting: ");
        int a = scanner.nextInt();

        for (int i = 0; i <= 10; i++) {
            System.out.println("Konfet "+i+"kg da:"+i*a+" so'm bo'ladi");
        }
    }
}

